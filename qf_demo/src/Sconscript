#!python
Import('env')

penv = env.Clone()
penv.Append(CPPPATH=['./'])

r = {
    'libs' : [],
    'path' : []
}
r['path'].append(Dir('./'))

src = env.FindFiles('./', '*.c')
lib = penv.StaticLibrary('qf_demo', src)

Depends(env.SourceToObjects(src), env['CPPINCLUDES'])

r['libs'].append(lib)
Return('r')
